{
    "version": 2,
    "builds": [
        {
            "src": "SHOPNIQ/wsgi.py",
            "use": "@vercel/python-server",
            "config": {
                "maxLambdaSize": "150mb",
                "maxDuration": 120
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "SHOPNIQ/wsgi.py"
        }
    ]
}
